{
    "extends": "airbnb/legacy",
    "rules": {
        "strict": [2, "global"],
        "no-multi-spaces": 0,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "padded-blocks": 0,
        "no-else-return": 0,
        "no-unused-vars": 0
    },
    "env": {
        "node": true
    }
}
